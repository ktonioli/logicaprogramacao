programa {
  funcao inicio() {
    
    inteiro numero 

    escreva("Digite um número qualquer:")
    leia (numero)

    escolha (numero) {
    caso 0 : 
         escreva ("No valor é 0")
         pare
    caso 1: 
         escreva ("O valor é 1")
         pare
    caso 2: 
         escreva ("O valor é 2")
         pare 
    caso contrario:
         escreva ("O valor não é igual a 0, 1 ou 2")

        
    }
  }
}
